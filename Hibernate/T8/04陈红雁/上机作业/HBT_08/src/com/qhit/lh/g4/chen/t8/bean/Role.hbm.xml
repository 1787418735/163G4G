<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
<class name="com.qhit.lh.g4.chen.t8.bean.Role" table="t_role" schema="dbo" catalog="db_emp">
<id name="roleId" type="java.lang.integer">
<column name="roleId"/>
<generator class="native">
</generator>
</id>
<property name="roleName" type="java.lang.string" >
<column name="roleName" length="25" not-null="true"></column>
</property>
<property name="memo" type="java.lang.string" >
<column name="memo" length="25" not-null="true"></column>
</property>

<set name="roles" table="role_user" lazy="false">
<key column="userId"></key>
<many-to-many class="com.qhit.lh.g4.chen.t8.bean.Role" column="roleId"></many-to-many>
</set>
</class>
</hibernate-mapping>
