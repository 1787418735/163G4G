<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="com.xjl.t6.bean.User" table="emp">
		<id name="id" column="id" type="integer">
			<generator class="native"></generator>
		</id>
		<property name="uname" column="uname" type="string"></property>
		<property name="upwd" column="upwd" type="string"></property>
		<property name="birthday" column="birthday" type="string"></property>
		<property name="sex" column="sex" type="string"></property>

	</class>

</hibernate-mapping>
