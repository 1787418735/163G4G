<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="com.qhit.lh.g4.tmx.t1.bean.User" table="t_user">
		<id name="id" column="id" type="integer">
			<generator class="native">
			
			</generator>
		</id>
		<property name="uanem" column="uname" type="String"></property>
		<property name="upwd" column="upwd" type="String"></property>
		<property name="birthdat" column="birthdat" type="String"></property>
		<property name="sex" column="sex" type="String"></property>
		<property name="enable" column="enable" type="integer"></property>
	</class>
</hibernate-mapping>
