<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
<class name="com.qhit.lh.g4.chen.t2.bean.UserInfo" table="t_userInfo">
<id name="userId" column="userId" type="integer">
<generator class="foreign">
<param name="property">emp</param>
</generator>
</id>
<property name="uname" column="uname" type="string" ></property>
<property name="psaaword" column="psaaword" type="string" ></property>

<one-to-one name="emp" class="com.qhit.lh.g4.chen.t2.bean.Emp" cascade="all"></one-to-one>
</class>
</hibernate-mapping>
