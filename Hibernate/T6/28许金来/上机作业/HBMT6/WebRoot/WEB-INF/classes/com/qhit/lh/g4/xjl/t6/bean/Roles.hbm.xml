<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="com.qhit.lh.g4.xjl.t6.bean.Roles" table="roles">
		<id name="roleId" column="roleId" type="integer">
			<generator class="native"></generator>
		</id>
		<property name="roleName" column="roleName" type="string"></property>
		<property name="memo" column="memo" type="string"></property>
		
		<set name="users" table="roles_userInfo" lazy="false">
			<key column="roleId"></key>
			<many-to-many class="com.qhit.lh.g4.xjl.t6.bean.UserInfo" column="userId"></many-to-many>
		</set>
	</class>
</hibernate-mapping>
