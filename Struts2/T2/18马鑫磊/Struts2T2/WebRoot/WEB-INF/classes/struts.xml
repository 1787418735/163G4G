<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
    "http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
	<package name="userManager" extends="struts-default">
	<interceptors>
		<interceptor name="myinterceport" class="com.jack.struts.CheckRightInterceptor"></interceptor>
	</interceptors>
		<action name="user" class="com.jack.struts.UserAction">
			<interceptor-ref name="myinterceport"></interceptor-ref>
			<interceptor-ref name="timer"></interceptor-ref>
			<interceptor-ref name="params"></interceptor-ref>
			<result name="success">/jsp/success.jsp</result>
		</action>
	</package>
</struts>