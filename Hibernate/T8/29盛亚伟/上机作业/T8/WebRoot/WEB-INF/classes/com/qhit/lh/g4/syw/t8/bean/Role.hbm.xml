<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="com.qhit.lh.g4.syw.t8.bean.Role" table="t_role">
		<!-- 主键的映射  -->
		<id name="roleId" column="userId" type="integer">
			<column name="roleId"></column>
			<generator class="native"></generator>
		</id>
		<!-- 普通成员属性的映射 -->
		<property name="roleName" column="roleName" type="string"></property>
		<property name="memo" column="memo" type="string"></property>
		<!-- 一对一的关系映射 -->
		<one-to-one name="emp" class="com.qhit.lh.g4.syw.t8.bean.Emp" cascade="all"></one-to-one>
		<!-- 多对多的关系映射 -->
		<set name="user" table="role_user" lazy="false">
			<key column="userId"></key>
			<many-to-many class="com.qhit.lh.g4.syw.t8.bean.Role" column="userId"></many-to-many>
		</set>
	</class>
</hibernate-mapping>
